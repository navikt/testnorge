<settings>

    <profiles>
        <profile>
            <activation>
                <activeByDefault>true</activeByDefault>
            </activation>
            <repositories>
                <repository>
                    <id>XWiki</id>
                    <url>https://maven.xwiki.org/externals/</url>
                </repository>
            </repositories>
        </profile>
    </profiles>

    <servers>
        <server>
            <id>github-package-registry-navikt</id>
            <username>token</username>
            <password>${env.NAV_TOKEN}</password>
        </server>
    </servers>
</settings>
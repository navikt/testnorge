    </xs:complexType>
    <xs:complexType name="HentUtvidetPersonstatusResponseType">
        <xs:sequence>
            <xs:element name="Resultat" type="HentUtvidetPersonstatusResultat"/>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="HentPersonstatusResultat">
        <xs:annotation>
            <xs:documentation>Resultatsett fra operasjon for henting av oppholdsstatus</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="Uttrekkstidspunkt" type="xs:dateTime" nillable="false">
                <xs:annotation>
                    <xs:documentation>Angir dato og klokkeslett for når uttrekket ble gjort hos UDI. Elementet returneres alltid, uavhengig av inkluderings-flagg. Inngår i tillegg T006</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="Foresporselsfodselsnummer" type="common:Fodselsnummer">
                <xs:annotation>
                    <xs:documentation>Fødselsnummer / D-nummer det ble spurt etter. Kan være annet fødselsnummer / D-nummer enn gjeldende person sitt fødselsnummer / D-nummer hvis man spør med alias fødselsnummer / D-nummer. Elementet returneres alltid, uavhengig av inkluderings-flagg. Inngår i tillegg T005</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="GjeldendePerson" type="GjeldendePerson">
                <xs:annotation>
                    <xs:documentation>Angir personalia til gjeldende person som ble funnet og benyttet i uttrekk. Elementet returneres alltid, uavhengig av inkluderings-flagg. Inngår i krav F001, F002, samt tillegg T005</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="GjeldendeOppholdsstatus" type="GjeldendeOppholdsstatus" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Angir oppholdsstatus på gjeldende person. Elementet returneres alltid, uavhengig av inkluderings-flagg. Inngår i krav F004, F005, F016</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="Arbeidsadgang" type="Arbeidsadgang" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Angir gjeldende person sin arbeidsadgang. Inkluderes i respons hvis InkluderArbeidsadgang i HentPersonstatusParameter er satt til true. Inngår i krav F012</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="Avgjorelsehistorikk" type="Avgjorelser" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Angir gjeldende person sin avgjørelsehistorikk. Inkluderes i respons hvis InkluderAvgjorelsehistorikk i HentPersonstatusParameter er satt til true. Inngår i krav F003, F006, F010, F011, F014, F018, F023, F024, samt tilleggene T002, T003, T004, T008, T009, T010, T011, T013, T014</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="UavklartFlyktningstatus" type="xs:boolean" nillable="true">
                <xs:annotation>

  "compilerOptions": {
    "jsx": "react",
    "sourceMap": true,
    "module": "ESNext",
    "declaration": true,
    "moduleResolution": "Node",
    "skipLibCheck": true,
    "target": "esnext",
    "typeRoots": [
      "./node_modules/@types"
    ],
    "esModuleInterop": true
  },
  "exclude": [
    "node_modules",
    "build"
  ]
}
